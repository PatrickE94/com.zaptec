{
  "name": {
    "en": "Charger",
    "no": "Lader"
  },
  "height": 140,
  "settings": [],
  "devices": {
    "type": "app",
    "singular": true,
    "filter": {
      "class": "evcharger",
      "capabilities": "cable_permanent_lock"
    }
  },
  "api": {
    "getChargerState": {
      "method": "GET",
      "path": "/"
    },
    "lockCharger": {
      "method": "POST",
      "path": "/"
    },
    "setAuthenticationRequirement": {
      "method": "PUT",
      "path": "/auth"
    },
    "setChargingMode": {
      "method": "PUT",
      "path": "/charging-mode"
    }
  }
}